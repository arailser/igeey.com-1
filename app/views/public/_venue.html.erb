  <div class="event_type" title=''></div>
  <div class="event_box">
    <%= link_to image_tag(venue.cover.url(:_80x60),:class => "venue_thumb_cover"),venue,:style=>'float:left'%>
    <div class="event" style="margin-left:96px;margin-top:-5px;width:510px">
      <div class="event_panel" style="width:488px">
        <%= link_to '行动召集',"#{new_calling_path}?venue_id=#{venue.id}",:class => 'open_dialog',:title => '行动召集'%>
        　<%= link_to '记录行动',"#{new_record_path}?venue_id=#{venue.id}",:class => 'open_dialog',:title => '记录行动'%>
        　<%= link_to '更新信息',"#{edit_venue_path(venue)}?back_path=#{request.path}"%>
      </div>
      
      <h5><%= link_to venue.name ,venue%> <span><%= venue.geo.name %></span></h5>
      <div class="explanation" style="margin-top:-4px"><%= venue.intro%></div>
    </div>
  </div>
