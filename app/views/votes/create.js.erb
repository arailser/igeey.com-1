<% unless @vote.errors.empty? %>
  alert('<%= @vote.errors.map{|msg| msg[1]} %>')
<% else %>
  $('#<%= "#{@voteable.class.to_s.downcase}_vote_#{@voteable.id}"%>').html('<%= escape_javascript(raw("#{image_tag('/images/icon/upgrey.png',:class => 'icon')} #{@vote.voteable.votes.count}")) %>')
<% end %>
